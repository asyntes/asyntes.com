{
  "hosting": [
    {
      "target": "main-site",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "redirect-site",
      "redirects": [
        {
          "source": "/**",
          "destination": "https://asyntes.com/**",
          "type": 301
        }
      ]
    }
  ]
}